{
  "name": "wss",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint --fix . --ext .ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run ./db/migrate.ts",
    "db:seed": "bun run ./db/seed.ts",
    "db:reset": "bun run ./db/reset.ts",
    "db:studio": "drizzle-kit studio",
    "p:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "p:s": "prisma studio",
    "p:pull": "prisma db pull",
    "p:push": "prisma db push",
    "p:g": "prisma generate",
    "p:v": "prisma validate",
    "p:f": "prisma format",
    "p:m": "prisma migrate dev"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "express": "^4.19.2",
    "socket.io": "^4.7.5",
    "@prisma/client": "5.18.0",
    "axios": "1.7.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "@types/express": "^4.17.21",
    "@types/node": "^22.3.0",
    "tslint": "^6.1.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prisma": "5.18.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "^1.1.4",
    "@types/jsonwebtoken": "^9.0.6",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
